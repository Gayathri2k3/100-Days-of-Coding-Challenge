import java.util.*;
public class IfCond {
    int a = 10;
    int b = 20;
   void checkValues() {
        if (a < b) {
            System.out.println("A is Smaller");
        }
    }
public static void main(String args[]) {
        IfCond condition = new IfCond();
        condition.checkValues();
    }
}
