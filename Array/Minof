public class Minarr{
    public static void main(String args[]){
        int a[]=new int[5];
        a[0]=110;
        a[1]=70;
        a[2]=64;
        a[3]=87;
        a[4]=90;
        int min=a[0];
        for(int i=1;i<a.length;i++){
            if(a[i]<min)
            min=a[i];
            else
            min=a[0];
        }
        System.out.println("The Min of the array is :"+min);
    }
} 
