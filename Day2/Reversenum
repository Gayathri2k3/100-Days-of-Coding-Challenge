import java.util.Scanner;
public class Reversenum{
    public static void main (String args[]){
        int num=123;
        int rev=0;
        while(num!=0){
            int y=num%10;
            rev=(rev*10)+y;
            num=num/10;
        }
        System.out.println("Reverse of the given number="+rev);
    }
}
