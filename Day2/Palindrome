import java.util.Scanner;
public class Palindrome{
    public static void main(String args[]){
        int n=656;
        int a,sum=0;
        int temp=n;
        while(n>0){
        a=n%10;
        sum=(sum*10)+a;
        n=n/10;
        }
        if(temp==sum)
        System.out.println("Palindrome");
        else
        System.out.println("Not a Palindrome");

        
    }
}
