public class Maxarr{
    public static void main(String args[]){
        int a[]=new int[5];
        a[0]=80;
        a[1]=42;
        a[2]=54;
        a[3]=83;
        a[4]=90;
        int max=a[0];
        for(int i=1;i<a.length;i++){
            if(a[i]>max)
            max=a[i];
            else
            max=a[0];
        }
        System.out.println("The Maxi of the array is :"+max);
    }
} 
